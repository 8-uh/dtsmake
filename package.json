{
  "name": "dtsmake",
  "version": "0.0.7",
  "description": "TypeScript d.ts file generator from JavaScript files",
  "main": "lib/dtsmake.js",
  "bin": "bin/dtsmake",
  "scripts": {
    "test": "mocha --compilers ts:espower-typescript/guess test/*-test.ts",
    "testw": "mocha --compilers ts:espower-typescript/guess -w test/*-test.ts",
    "extest": "(cd example && npm install --save-dev ../ && node example.js)",
    "build": "gulp build"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/ConquestArrow/dtsmake/"
  },
  "bugs": {
    "url": "https://github.com/ConquestArrow/dtsmake/issues"
  },
  "engines": {
    "npm": "2.x.x - 3.x.x"
  },
  "keywords": [
    "TypeScript",
    "JavaScript",
    "generator"
  ],
  "author": "ConquestArrow",
  "license": "MIT",
  "private": false,
  "devDependencies": {
    "@types/clone": "^0.1.30",
    "@types/commander": "^2.3.31",
    "@types/empower": "^1.2.30",
    "@types/gulp": "^3.8.32",
    "@types/gulp-concat": "0.0.29",
    "@types/gulp-typescript": "0.0.32",
    "@types/mocha": "^2.2.33",
    "@types/node": "^6.0.52",
    "@types/power-assert": "^1.4.29",
    "@types/power-assert-formatter": "^1.4.28",
    "@types/sinon": "^1.16.33",
    "espower-typescript": "^5.0.1",
    "gulp": "^3.9.1",
    "gulp-concat": "^2.6.0",
    "gulp-header": "^1.8.7",
    "gulp-release-tasks": "0.0.3",
    "gulp-todo": "^4.1.0",
    "gulp-typescript": "^2.13.6",
    "mocha": "^2.5.3",
    "power-assert": "^1.4.1",
    "sinon": "^1.17.4",
    "typescript-require": "^0.2.9-1"
  },
  "dependencies": {
    "clone": "^1.0.2",
    "commander": "^2.9.0",
    "tern": "^0.19.0"
  }
}
