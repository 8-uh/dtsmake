{
  "name": "dtsmake",
  "version": "0.0.3",
  "description": "TypeScript d.ts file generator from JavaScript files",
  "main": "lib/dtsmake.js",
  "bin": "bin/dtsmake",
  "scripts": {
    "test": "mocha --compilers ts:espower-typescript/guess test/*-test.ts",
    "testw": "mocha --compilers ts:espower-typescript/guess -w test/*-test.ts",
    "extest": "(cd example && npm install --save-dev ../ && node example.js)",
    "build": "tsc"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/ConquestArrow/dtsmake/"
  },
  "keywords": [
    "TypeScript"
  ],
  "author": "ConquestArrow",
  "license": "MIT",
  "private": false,
  "devDependencies": {
    "espower-typescript": "^1.0.0",
    "gulp": "^3.9.0",
    "gulp-concat": "^2.6.0",
    "gulp-header": "^1.2.2",
    "gulp-todo": "^3.3.0",
    "gulp-typescript": "^2.8.0",
    "mocha": "^2.2.5",
    "power-assert": "^0.11.0",
    "sinon": "^1.15.4",
    "tsd": "^0.6.3",
    "typescript-require": "^0.2.9-1"
  },
  "dependencies": {
    "clone": "^1.0.2",
    "commander": "^2.8.1",
    "tern": "^0.13.0"
  }
}
